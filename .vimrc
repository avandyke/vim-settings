set background=dark
syntax enable
try
	colorscheme monokai
	set t_Co=256
catch
endtry
set hidden
set tabstop=4
set autoindent
set copyindent
set number
set shiftwidth=4
set showmatch
set ignorecase
set smarttab
set hlsearch
set incsearch
set history=300
set undolevels=1000
filetype plugin indent on
set pastetoggle=<F2>
set mouse=a
